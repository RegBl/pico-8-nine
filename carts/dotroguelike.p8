--core

function _init()
	t=0
	dirx={-1,1,0,0,1,1,-1,-1}
	diry={0,0,-1,1,-1,1,1,-1}
	
	wlksnd={0,1,2}
	
	startgame()
end

function _update60()
	t+=1
	_upd()
end

function _draw()
	_drw()
end

function startgame()
	btnbuff=-1
	mob={}
	
	p_mob=addmob(1,1,1)
	p_t=0
	
	music(01)
	_upd=update_game
	_drw=draw_game
end
-->8
--update

function update_game()
	buffbtn()
	dobtn(btnbuff)
	btnbuff=-1
end

function update_pturn()
	buffbtn()	
	p_t=min(p_t+0.125,1)
	
	p_mob.mov(p_mob,p_t)
	
	if (p_t==1)	_upd=update_game
end

function buffbtn()
	if (btnbuff==-1) btnbuff=getbtn()
end

function getbtn()
	for i=0,5 do
		if (btnp(i)) return i
	end
	return -1
end

function dobtn(btn)
	if (btn<0) return
	if (btn<4) moveplayer(dirx[btn+1],diry[btn+1])
end
