--mobs

function addmob(typ,mx,my)
	local m={
		x=mx,
		y=my,
		flash=0,
	}
	add(mob,m)
	return m
end

function mobwalk(mb,dx,dy)
	mb.x+=dx
	mb.y+=dy
	mb.sox,mb.soy=-dx,-dy
	mb.ox,mb.oy=mb.sox,mb.soy
	mb.mov=mov_walk
end

function mobbump(mb,dx,dy)
	mb.sox,mb.soy=dx,dy
	mb.ox,mb.oy=0,0
	mb.mov=mov_bmp
end

function mov_walk(mb,at)
	mb.ox=mb.sox*(1-at)
	mb.oy=mb.soy*(1-at)
end

function mov_bmp(mb,at)
	local tme=at
	
	if (at>0.5) tme=1-at
	
	mb.ox=mb.sox*tme
	mb.oy=mb.soy*tme
end
