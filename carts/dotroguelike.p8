--draw

function draw_game()
	cls()
	--draw map
	test_level()
	draw_mob(p_mob)
end

function draw_mob(m)
	local col=10
	pset(m.x,m.y,col)
end

function test_level()
	color(5)
	rectfill(0,0,127,127)
	color(0)
	rectfill(1,1,10,10)
	rectfill(12,1,21,5)
	rectfill(4,21,8,25)
	pset(11,3)
	line(6,11,6,20)
	line(6,15,16,15)
	line(16,15,16,6)
end
