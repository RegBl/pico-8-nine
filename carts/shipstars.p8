__lua__
-- ship and stars
-- by sonofthe

function _init()
	stars={}
	ship={
		x=64,
		y=64
	}
	
	for i=0,15 do
		add(stars,getnewstar())
	end
	
	sfx(0,1)
end

function _update()
	for star in all(stars) do
		if star.y>128 then
			del(stars,star)
			add(stars,getnewstar())
		else
			star.y+=star.speed
		end
	end
	
	shipmv()
	shipsnd()
	
	if ship.x<0 then ship.x=0 end
	if ship.x>119 then ship.x=119 end
	if ship.y<0 then ship.y=0 end
	if ship.y>117 then ship.y=117 end
	
end

function _draw()
	cls(0)
	color(7)
	for star in all(stars) do
		line(star.x,star.y,star.x,star.y+star.speed)
	end
	drawship(ship.x,ship.y)
end

function getnewstar()
		local star={
			x=rnd(128),
			y=0,
			speed=rnd(3)
		}
		return star
end

function shipmv()
	if btn(⬅️) then 
		ship.x-=2
	end
	if btn(➡️) then 
		ship.x+=2
	end
	if btn(⬆️) then 
		ship.y-=3
	end
	if btn(⬇️) then 
		ship.y+=1
	end
end
-->8
--draw

function drawship(x,y)
	color(6)
	line(x,y+10,x+8,y+10)
	line(x+8,y+10,x+4,y)
	line(x+4,y,x,y+10)
	pset(x+3,y+11,10)
	pset(x+4,y+11,8)
	pset(x+5,y+11,10)
	pset(x+4,y+12,10)
	pset(x+4,y+12+(rnd(2)),10)
end

function shipsnd()
	sfx(0,-1)
	sfx(1,-2)
	sfx(2,-2)
	if (btn(⬅️) or btn(➡️)) then
		sfx(0,1)
		sfx(1,2)
	end
	if (btn(⬆️)) then
		sfx(0,1)
		sfx(2,2)
	end
	sfx(0,1)	
end
